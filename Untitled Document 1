function modifier($mod){
    $text="<div id=package>
            <div id=packageName> 
                <strong>$mod[0] </strong></br> $mod[1]
            </div>
            <div id=packageButton> 
                <button type=\"button\" id=\"buttonN\" onclick=\"showPrevious()\">Previous</button>
            </div>
            </div></br>";
    return $text;
